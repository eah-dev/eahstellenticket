						<TCEforms>
							<label>LLL:EXT:eahstellenticket/Resources/Private/Language/locallang_be.xlf:flexform.show.eahj_ausb</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.showeahjausb>
				</el>
			</ROOT>
		</sDEF>
		<!--
		################################
			SHEET Template
		################################
		-->
		<template>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:eahstellenticket/Resources/Private/Language/locallang_be.xlf:flexform.tab.template</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>

					<!-- Template Layout - List -->
					<settings.templateLayoutList>
