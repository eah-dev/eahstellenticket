							<label>LLL:EXT:eahstellenticket/Resources/Private/Language/locallang_be.xlf:flexforms_template.selectlayout_list</label>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:eahstellenticket/Resources/Private/Language/locallang_be.xlf:flexforms_template.selectlayout_list.style1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:eahstellenticket/Resources/Private/Language/locallang_be.xlf:flexforms_template.selectlayout_list.style2</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
								</items>
							</config>
