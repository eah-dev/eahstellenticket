							</config>
						</TCEforms>
					</settings.Url>

					<!-- enable showeahjprof -->
					<settings.showeahjprof>
						<TCEforms>
							<label>LLL:EXT:eahstellenticket/Resources/Private/Language/locallang_be.xlf:flexform.show.eahj_prof</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.showeahjprof>

					<!-- enable showeahjwiss -->
					<settings.showeahjwiss>
						<TCEforms>
							<label>LLL:EXT:eahstellenticket/Resources/Private/Language/locallang_be.xlf:flexform.show.eahj_wiss</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.showeahjwiss>

					<!-- enable showeahjverw -->
					<settings.showeahjverw>
						<TCEforms>
							<label>LLL:EXT:eahstellenticket/Resources/Private/Language/locallang_be.xlf:flexform.show.eahj_verwalt</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.showeahjverw>

					<!-- enable showeahjausb -->
